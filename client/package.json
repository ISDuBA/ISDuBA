{
  "name": "isduba-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:test": "vite build --config vite.test-config.js",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "start": "vite",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --cache . && eslint --cache .",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write .",
    "test:unit": "vitest run",
    "test:integration": "playwright test",
    "cover": "npm run cover:unit && npm run cover:integration",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:integration": "nyc --silent --no-clean npm run test:integration",
    "prepare": "cd .. && husky client/.husky"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.47.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/eslint": "9.6.1",
    "@types/node": "^24.8.1",
    "@types/qs": "^6.14.0",
    "@types/sortablejs": "^1.15.8",
    "@types/validator": "^13.15.4",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-v8": "^4.0.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-istanbul": "^7.0.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^2.46.1",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.13.8",
    "husky": "^9.1.7",
    "monocart-reporter": "^2.9.23",
    "nyc": "^17.1.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "source-map-support": "^0.5.21",
    "svelte": "^5.41.1",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.2"
  },
  "type": "module",
  "dependencies": {
    "@playwright/experimental-ct-svelte": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/chroma-js": "^3.1.1",
    "boxicons": "^2.1.4",
    "chart.js": "^4.5.1",
    "chartjs-adapter-moment": "^1.0.1",
    "chroma-js": "^3.1.2",
    "debounce": "^2.2.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "oidc-client-ts": "^3.3.0",
    "qs": "^6.14.0",
    "sortablejs": "^1.15.6",
    "svelte-spa-router": "^4.0.1",
    "validator": "^13.15.20"
  },
  "overrides": {
    "@sveltejs/kit": {
      "cookie": "0.7.2"
    }
  }
}

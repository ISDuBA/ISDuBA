{
  "name": "isduba-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:test": "vite build --config vite.test-config.js",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "start": "vite",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --cache . && eslint --cache .",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write .",
    "test:unit": "vitest run",
    "test:integration": "playwright test",
    "cover": "npm run cover:unit && npm run cover:integration",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:integration": "nyc --silent --no-clean npm run test:integration",
    "prepare": "cd .. && husky client/.husky"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.50.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "9.6.1",
    "@types/node": "^22.10.10",
    "@types/qs": "^6.9.18",
    "@types/sortablejs": "^1.15.8",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "babel-plugin-istanbul": "^7.0.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "flowbite": "^2.5.2",
    "flowbite-svelte": "^0.47.4",
    "husky": "^9.1.7",
    "monocart-reporter": "^2.9.13",
    "nyc": "^17.1.0",
    "postcss": "^8.5.1",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "source-map-support": "^0.5.21",
    "svelte": "^4.2.19",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^4.0.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vitest": "^2.0.5"
  },
  "type": "module",
  "dependencies": {
    "@playwright/experimental-ct-svelte": "^1.50.0",
    "@types/chroma-js": "^3.1.0",
    "boxicons": "^2.1.4",
    "chart.js": "^4.4.7",
    "chartjs-adapter-moment": "^1.0.1",
    "chroma-js": "^3.1.2",
    "debounce": "^2.2.0",
    "dompurify": "^3.2.3",
    "jwt-decode": "^4.0.0",
    "marked": "^15.0.6",
    "moment": "^2.30.1",
    "oidc-client-ts": "^3.1.0",
    "qs": "^6.14.0",
    "sortablejs": "^1.15.6",
    "svelte-spa-router": "^4.0.1",
    "validator": "^13.12.0"
  }
}

{
  "extends": "@istanbuljs/nyc-config-typescript",
  "extension": [".svelte", ".html", ".ts"],
  "exclude": [
    "*.config.js",
    "**/*spec.js",
    "**/*.d.ts",
    ".eslintrc.js",
    "node_modules",
    ".svelte-kit",
    "coverage/**",
    "babel.config.js"
  ],
  "reporter": ["html", "text", "lcov"],
  "compact": false,
  "all": true
}
